<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="15dp"
    tools:context="com.example.funtaipei.travelCollection.TravelCollectionFragment">



        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/travelCollectionRecycleview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/travelCollection_Memberimageview"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="20dp">

        </androidx.recyclerview.widget.RecyclerView>


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/travelCollectionbtnAdd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|bottom"
        android:backgroundTint="#3F51B5"
        android:src="@android:drawable/ic_input_add"></com.google.android.material.floatingactionbutton.FloatingActionButton>


</androidx.coordinatorlayout.widget.CoordinatorLayout>